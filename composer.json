"repositories": [
    {
        "type": "package",
        "package": {
            "name": "Wordpress/Coding Standard",
            "version": "dev-master",
            "source": {
                "url": "https://github.com/shashank-infobeans/wp-ruleset.git",
                "type": "git",
                "reference": "master"
            },
            "autoload": {
        "psr-4": {
            "CustomHook\\PreCommitHook\\": "hook-install/"
        }
    },
    "extra": {
        "class": "CustomHook\\PreCommitHook\\Hook"
    },
    "scripts": {
        "post-update-cmd": [
            "CustomHook\\PreCommitHook\\Hook::run"
        ]
    }
        }
    }
]
